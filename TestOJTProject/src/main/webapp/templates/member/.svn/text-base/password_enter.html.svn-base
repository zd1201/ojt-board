<!-- sub_area -->
<div id="sub_area">
	<div class="title_area">
		<div class="title">비밀번호 재입력</div>
	</div>
<div class="viewlist" style="padding: 10px 15px 0px 30px;">
		<form name="form1" style="text-align:center">
			<div>
				<ul>
					<li style="line-height:31px;"><span class="l_01" style="font-size : 17px;">비밀번호를 입력해주세요.</span></li>
					<li style="line-height:4px;"><input type="password" name="textfield" type="password" style="width: 150px;" maxlength="16" ng-model="pwdCheck" ng-enter="DoWork()"></li>
					<li style="line-height:10px;">
					<button type="button" class="btn btn-primary" ng-click="ok()">확인</button>
					<button type="button" class="btn btn-default" ng-click="cancel()">취소</button>
					</li>
				</ul>
			</div>
		</form>
	</div>
</div>